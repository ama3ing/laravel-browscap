{
    "name": "propa/laravel-browscap",
    "description": "Browscap-PHP integration for Laravel 5",
    "keywords": ["laravel", "browscap", "browscap-php", "user-agent", "browser"],
    "version": "2.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Kulbakin",
            "email": "p.kulbakin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0,<7.3.0",
        "illuminate/support": "~5.0",
        "browscap/browscap-php": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Propa\\BrowscapPHP\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Propa\\BrowscapPHP\\BrowscapServiceProvider"
            ],
            "aliases": {
                "Browscap": "Propa\\BrowscapPHP\\Facades\\Browscap"
            }
        }
    }
}
